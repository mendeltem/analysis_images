import numpy as np
from random import gauss
from numpy.random import seed
from numpy.random import randint
from numpy.random import randn
import matplotlib.pyplot as plt

index = 100000
seed(1)

random_numbers = randn(index)
scaled_value = np.round(400 + random_numbers * 10)

random_numbers_2 = randn(index)
scaled_value_2 = np.round(400 + random_numbers_2 * 10)

print(scaled_value_2)

plt.hist(scaled_value, bins =100)


diff = scaled_value - scaled_value_2




name = ["mendel"]
alias = ["mendula"]


dict(zip(name, alias))

name[0]

food = ('apple', 'orange')
d = dict.fromkeys(food, "Mendel")
print(d)

d.items()
d.keys()

